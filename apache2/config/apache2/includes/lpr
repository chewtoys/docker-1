# AIS-2400 include file
Include includes/protect_internal_files

# alias static to vhost
Alias /static /var/www/lpr/current/static

# alias for lpr
Alias /lpr /var/www/lpr/current

<IfModule mod_rewrite.c>
    RewriteEngine  On
    RewriteCond    %{REQUEST_URI}   ^/lpr/.* [NC]
    RewriteRule    /lpr/(.*)/?      /lpr/render.php?file=$1     [QSA,PT,L]
</IfModule>
