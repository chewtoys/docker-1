# settings for all vhosts/domains
<Location />
	# Requires mod_expires to be enabled.
	<IfModule mod_expires.c>
		# Enable expirations.
		ExpiresActive	On

		# We have multiple apache server so we can't use default ETag with includes INode...
		FileETag	MTime Size

		# Cache all files for 4 weeks after access.
		ExpiresDefault	"access plus 4 weeks"
		#ExpiresDefault	"access plus 1 hours"

		# Do not cache dynamically generated html pages.
		ExpiresByType	text/xml	access
		ExpiresByType	text/html	access
		ExpiresByType	text/plain	access
#		ExpiresByType	text/javascript	access
#		ExpiresByType	application/javascript	access
#		ExpiresByType	text/css	access
	</IfModule>
</Location>
